<odoo>
    <data>
        <template id="hello">
            <t t-call="web.html_container">
                <h1 t-out="title"/>

                <form method="POST">
                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
                    <input name="my_input" />
                    <input name="date" type="date"/>
                    <input type="submit"/>
                </form>
                <script>
                    var odoo = <t t-out="odoo"/>
                </script>
            </t>
        </template>
    </data>
</odoo>